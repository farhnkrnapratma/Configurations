{
    "assistant": {
        "default_model": {
            "provider": "google",
            "model": "gemini-1.5-flash"
        },
        "version": "2"
    },
    "magnification": 2.0,
    "border_size": 0.0,
    "inactive_opacity": 1.0,
    "auto_install_extensions": {
        "html": true
    },
    "autosave": "on_focus_change",
    "restore_on_startup": "last_session",
    "autoscroll_on_clicks": true,
    "auto_update": true,
    "base_keymap": "VSCode",
    "buffer_font_family": "Ubuntu Sans Mono",
    "buffer_font_size": 14,
    "buffer_font_weight": 500,
    "buffer_line_height": "comfortable",
    "confirm_quit": true,
    "centered_layout": {
        "left_padding": 0.2,
        "right_padding": 0.2
    },
    "load_direnv": "direct",
    "inline_completions": {
        "disabled_globs": [".env"]
    },
    "disabled_in": ["comment", "string"],
    "current_line_highlight": "line",
    "lsp_highlight_debounce": true,
    "cursor_blink": true,
    "cursor_shape": "underline",
    "scrollbar": {
        "show": "auto",
        "cursors": true,
        "git_diff": true,
        "search_results": true,
        "selected_symbol": true,
        "diagnostics": true,
        "axes": {
            "horizontal": true,
            "vertical": true
        }
    },
    "tab_bar": {
        "show": true,
        "show_nav_history_buttons": true
    },
    "tabs": {
        "close_position": "right",
        "file_icons": true,
        "git_status": true,
        "activate_on_close": "history",
        "always_show_close_button": true
    },
    "toolbar": {
        "breadcrumbs": true,
        "quick_actions": true
    },
    "enable_language_server": true,
    "ensure_final_newline_on_save": true,
    "format_on_save": "on",
    "formatter": "auto",
    "code_actions_on_format": {},
    "use_autoclose": true,
    "always_treat_brackets_as_autoclosed": false,
    "file_scan_exclusions": [
        "**/.git",
        "**/.svn",
        "**/.hg",
        "**/.jj",
        "**/CVS",
        "**/.DS_Store",
        "**/Thumbs.db",
        "**/.classpath",
        "**/.settings"
    ],
    "file_types": {},
    "git": {
        "git_gutter": "tracked_files",
        "inline_blame": {
            "enabled": true
        }
    },
    "indent_guides": {
        "enabled": true,
        "line_width": 1,
        "active_line_width": 1,
        "coloring": "fixed",
        "background_coloring": "disabled"
    },
    "hard_tabs": false,
    "hover_popover_enabled": true,
    "inlay_hints": {
        "enabled": false,
        "show_type_hints": true,
        "show_parameter_hints": true,
        "show_other_hints": true,
        "show_background": false,
        "edit_debounce_ms": 700,
        "scroll_debounce_ms": 50
    },
    "journal": {
        "path": "~",
        "hour_format": "hour24"
    },
    "languages": {},
    "proxy": null,
    "preview_tabs": {
        "enabled": true,
        "enable_preview_from_file_finder": false,
        "enable_preview_from_code_navigation": false
    },
    "max_modal_width": "small",
    "preferred_line_length": 80,
    "projects_online_by_default": true,
    "remove_trailing_whitespace_on_save": true,
    "search": {
        "whole_word": true,
        "case_sensitive": true,
        "include_ignored": false,
        "regex": true
    },
    "show_call_status_icon": true,
    "show_completions_on_input": true,
    "show_completion_documentation": true,
    "show_inline_completions": true,
    "show_whitespaces": "selection",
    "soft_wrap": "none",
    "wrap_guides": [],
    "tab_size": 4,
    "telemetry": {
        "diagnostics": true,
        "metrics": true
    },
    "terminal": {
        "alternate_scroll": "on",
        "blinking": "on",
        "copy_on_select": true,
        "dock": "bottom",
        "env": {
            "TERM": "xterm-256color"
        },
        "font_family": "Ubuntu Sans Mono",
        "font_size": 13,
        "line_height": "comfortable",
        "option_as_meta": false,
        "button": false,
        "font_weight": 0,
        "toolbar": {
            "breadcrumbs": true
        },
        "working_directory": "current_project_directory"
    },
    "theme": {
        "mode": "system",
        "dark": "Catppuccin Mocha",
        "light": "Catppuccin Latte"
    },
    "vim_mode": true,
    "project_panel": {
        "button": true,
        "default_width": 240,
        "dock": "left",
        "file_icons": true,
        "folder_icons": true,
        "git_status": true,
        "indent_size": 20,
        "indent_guides": {
            "show": "always"
        },
        "auto_reveal_entries": true,
        "auto_fold_dirs": true,
        "scrollbar": {
            "show": "always"
        }
    },
    "unnecessary_code_fade": 0.3,
    "ui_font_family": "Ubuntu Sans Mono",
    "ui_font_size": 18,
    "ui_font_weight": 500
}
